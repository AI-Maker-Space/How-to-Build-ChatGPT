{
  "name": "chatgpt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:backend": "cd backend && uvicorn app:app --reload --port 8000",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "setup": "npm install && npm run install:backend"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "next": "15.5.4",
    "@react-oauth/google": "^0.12.1",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "zustand": "^5.0.0",
    "lucide-react": "^0.453.0",
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/js-cookie": "^3.0.6",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "@eslint/eslintrc": "^3",
    "concurrently": "^9.0.1"
  }
}
