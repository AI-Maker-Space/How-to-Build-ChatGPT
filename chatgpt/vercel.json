{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "functions": {
    "backend/app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-backend-url.com/api/:path*"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@backend_url",
    "NEXT_PUBLIC_GOOGLE_CLIENT_ID": "@google_client_id"
  }
}
